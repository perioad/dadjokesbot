{
  "name": "dadjokesbot",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "dadjokesbotPath": "src/lambdas/dadjokesbot",
    "sendjokesPath": "src/lambdas/send-jokes",
    "getjokePath": "src/lambdas/get-joke"
  },
  "scripts": {
    "do-checks": "tsc --noEmit",
    "build:dadjokesbot": "esbuild src/lambdas/dadjokesbot/index.ts --bundle --platform=node --outfile=src/lambdas/dadjokesbot/build/index.js",
    "build:sendjokes": "esbuild src/lambdas/send-jokes/index.ts --bundle --platform=node --outfile=src/lambdas/send-jokes/build/index.js",
    "build:getjoke": "esbuild src/lambdas/get-joke/index.ts --bundle --platform=node --outfile=src/lambdas/get-joke/build/index.js",
    "build:sendmessage": "esbuild src/lambdas/send-message/index.ts --bundle --platform=node --outfile=src/lambdas/send-message/build/index.js",
    "build:menu": "esbuild src/lambdas/menu/index.ts --bundle --platform=node --outfile=src/lambdas/menu/build/index.js",
    "build:announce": "esbuild src/lambdas/announce/index.ts --bundle --platform=node --outfile=src/lambdas/announce/build/index.js",
    "zip:dadjokesbot:win": "if exist \"src\\lambdas\\dadjokesbot\\build\\index.zip\" del \"src\\lambdas\\dadjokesbot\\build\\index.zip\" & powershell -command \"& {param($file); Compress-Archive -Path $file -DestinationPath 'src\\lambdas\\dadjokesbot\\build\\index.zip'}\" \"src\\lambdas\\dadjokesbot\\build\\index.js\"",
    "zip:getjoke:win": "if exist \"src\\lambdas\\get-joke\\build\\index.zip\" del \"src\\lambdas\\get-joke\\build\\index.zip\" & powershell -command \"& {param($file); Compress-Archive -Path $file -DestinationPath 'src\\lambdas\\get-joke\\build\\index.zip'}\" \"src\\lambdas\\get-joke\\build\\index.js\"",
    "zip:sendjokes:win": "if exist \"src\\lambdas\\send-jokes\\build\\index.zip\" del \"src\\lambdas\\send-jokes\\build\\index.zip\" & powershell -command \"& {param($file); Compress-Archive -Path $file -DestinationPath 'src\\lambdas\\send-jokes\\build\\index.zip'}\" \"src\\lambdas\\send-jokes\\build\\index.js\"",
    "zip:sendmessage:win": "if exist \"src\\lambdas\\send-message\\build\\index.zip\" del \"src\\lambdas\\send-message\\build\\index.zip\" & powershell -command \"& {param($file); Compress-Archive -Path $file -DestinationPath 'src\\lambdas\\send-message\\build\\index.zip'}\" \"src\\lambdas\\send-message\\build\\index.js\"",
    "zip:menu:win": "if exist \"src\\lambdas\\menu\\build\\index.zip\" del \"src\\lambdas\\menu\\build\\index.zip\" & powershell -command \"& {param($file); Compress-Archive -Path $file -DestinationPath 'src\\lambdas\\menu\\build\\index.zip'}\" \"src\\lambdas\\menu\\build\\index.js\"",
    "zip:announce:win": "if exist \"src\\lambdas\\announce\\build\\index.zip\" del \"src\\lambdas\\announce\\build\\index.zip\" & powershell -command \"& {param($file); Compress-Archive -Path $file -DestinationPath 'src\\lambdas\\announce\\build\\index.zip'}\" \"src\\lambdas\\announce\\build\\index.js\"",
    "zip:dadjokesbot:mac": "if [ -e \"src/lambdas/dadjokesbot/build/index.zip\" ]; then rm \"src/lambdas/dadjokesbot/build/index.zip\"; fi && zip -j \"src/lambdas/dadjokesbot/build/index.zip\" \"src/lambdas/dadjokesbot/build/index.js\"",
    "zip:sendjokes:mac": "if [ -e \"src/lambdas/send-jokes/build/index.zip\" ]; then rm \"src/lambdas/send-jokes/build/index.zip\"; fi && zip -j \"src/lambdas/send-jokes/build/index.zip\" \"src/lambdas/send-jokes/build/index.js\"",
    "zip:sendmessage:mac": "if [ -e \"src/lambdas/send-message/build/index.zip\" ]; then rm \"src/lambdas/send-message/build/index.zip\"; fi && zip -j \"src/lambdas/send-message/build/index.zip\" \"src/lambdas/send-message/build/index.js\"",
    "zip:getjoke:mac": "if [ -e \"src/lambdas/get-joke/build/index.zip\" ]; then rm \"src/lambdas/get-joke/build/index.zip\"; fi && zip -j \"src/lambdas/get-joke/build/index.zip\" \"src/lambdas/get-joke/build/index.js\"",
    "push-to-aws:dadjokesbot:win": "aws lambda update-function-code --function-name dadjokes-bot --zip-file fileb://src\\lambdas\\dadjokesbot\\build\\index.zip",
    "push-to-aws:getjoke:win": "aws lambda update-function-code --function-name dadjokes-getjoke --zip-file fileb://src\\lambdas\\get-joke\\build\\index.zip",
    "push-to-aws:sendmessage:win": "aws lambda update-function-code --function-name dadjokes-sendmessage --zip-file fileb://src\\lambdas\\send-message\\build\\index.zip",
    "push-to-aws:sendjokes:win": "aws lambda update-function-code --function-name dadjokes-sendjokes --zip-file fileb://src\\lambdas\\send-jokes\\build\\index.zip",
    "push-to-aws:menu:win": "aws lambda update-function-code --function-name dadjokes-menu --zip-file fileb://src\\lambdas\\menu\\build\\index.zip",
    "push-to-aws:announce:win": "aws lambda update-function-code --function-name dadjokes-announce --zip-file fileb://src\\lambdas\\announce\\build\\index.zip",
    "push-to-aws:dadjokesbot:mac": "aws lambda update-function-code --function-name dadjokes-bot --zip-file fileb://src/lambdas/dadjokesbot/build/index.zip",
    "push-to-aws:sendjokes:mac": "aws lambda update-function-code --function-name dadjokes-sendjokes --zip-file fileb://src/lambdas/send-jokes/build/index.zip",
    "push-to-aws:sendmessage:mac": "aws lambda update-function-code --function-name dadjokes-sendmessage --zip-file fileb://src/lambdas/send-message/build/index.zip",
    "push-to-aws:getjoke:mac": "aws lambda update-function-code --function-name dadjokes-getjoke --zip-file fileb://src/lambdas/get-joke/build/index.zip",
    "deploy:dadjokesbot:win": "npm run do-checks && npm run build:dadjokesbot && npm run zip:dadjokesbot:win && npm run push-to-aws:dadjokesbot:win",
    "deploy:getjoke:win": "npm run do-checks && npm run build:getjoke && npm run zip:getjoke:win && npm run push-to-aws:getjoke:win",
    "deploy:sendmessage:win": "npm run do-checks && npm run build:sendmessage && npm run zip:sendmessage:win && npm run push-to-aws:sendmessage:win",
    "deploy:sendjokes:win": "npm run do-checks && npm run build:sendjokes && npm run zip:sendjokes:win && npm run push-to-aws:sendjokes:win",
    "deploy:menu:win": "npm run do-checks && npm run build:menu && npm run zip:menu:win && npm run push-to-aws:menu:win",
    "deploy:announce:win": "npm run do-checks && npm run build:announce && npm run zip:announce:win && npm run push-to-aws:announce:win",
    "deploy:dadjokesbot:mac": "npm run do-checks && npm run build:dadjokesbot && npm run zip:dadjokesbot:mac && npm run push-to-aws:dadjokesbot:mac",
    "deploy:sendjokes:mac": "npm run do-checks && npm run build:sendjokes && npm run zip:sendjokes:mac && npm run push-to-aws:sendjokes:mac",
    "deploy:sendmessage:mac": "npm run do-checks && npm run build:sendmessage && npm run zip:sendmessage:mac && npm run push-to-aws:sendmessage:mac",
    "deploy:getjoke:mac": "npm run do-checks && npm run build:getjoke && npm run zip:getjoke:mac && npm run push-to-aws:getjoke:mac",
    "deploy:all:mac": "npm run deploy:dadjokesbot:mac && npm run deploy:sendjokes:mac && npm run deploy:sendmessage:mac && npm run deploy:getjoke:mac"
  },
  "author": "perioad",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/types": "^3.329.0",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.1",
    "esbuild": "0.17.19",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.335.0",
    "@aws-sdk/client-lambda": "^3.421.0",
    "@aws-sdk/lib-dynamodb": "^3.335.0",
    "elizabot": "^0.0.3",
    "grammy": "^1.20.3",
    "js-tiktoken": "^1.0.10",
    "lodash-es": "^4.17.21",
    "openai": "^4.83.0"
  }
}
